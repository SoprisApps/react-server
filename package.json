{
  "private": true,
  "scripts": {
    "audit": "npm run audit:prepare && npm run audit:run && npm run audit:clean",
    "audit:prepare": "./scripts/generate-package-lock-files.sh",
    "audit:prepare2": "lerna exec -- npm install --ignore-scripts --package-lock-only --dry-run",
    "audit:run": "npm audit --parseable || lerna exec --no-bail --stream -- npm audit --parseable",
    "audit:clean": "rimraf packages/*/package-lock.json",
    "bootstrap": "npm i",
    "bootstrap-no-i": "lerna bootstrap",
    "build": "lerna run build",
    "changelog": "lerna-changelog",
    "clean": "rimraf lerna-debug.log && lerna run clean",
    "debug": "cat lerna-debug.log && for d in packages/*/npm-debug.log*; do echo $d; cat $d; done",
    "lint": "lerna run lint",
    "nuke": "npm run clean && lerna clean --yes && rm -rf node_modules",
    "postinstall": "npm run bootstrap-no-i",
    "test": "lerna run test",
    "update-dependencies": "david u && lerna exec david u"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
